{
  "rewrites": [
    {
      "source": "/home",
      "destination": "/api/index"
    },
    {
      "source": "/home/",
      "destination": "/api/index"
    },
    {
      "source": "/home/letter/:letter/page/:page",
      "destination": "/api/letter?letter=:letter&page=:page"
    },
    {
      "source": "/home/letter/:letter/page/:page/",
      "destination": "/api/letter?letter=:letter&page=:page"
    },
    {
      "source": "/home/letter/:letter",
      "destination": "/api/letter?letter=:letter"
    },
    {
      "source": "/home/letter/:letter/",
      "destination": "/api/letter?letter=:letter"
    },
    {
      "source": "/category/:path+/page/:page",
      "destination": "/api/category?path=:path&page=:page"
    },
    {
      "source": "/category/:path+/page/:page/",
      "destination": "/api/category?path=:path&page=:page"
    },
    {
      "source": "/category/:path+",
      "destination": "/api/category?path=:path"
    },
    {
      "source": "/category/:path+/",
      "destination": "/api/category?path=:path"
    },
    {
      "source": "/series/page/:page",
      "destination": "/api/series_page?page=:page"
    },
    {
      "source": "/series/page/:page/",
      "destination": "/api/series_page?page=:page"
    },
    {
      "source": "/series",
      "destination": "/api/series_page"
    },
    {
      "source": "/series/",
      "destination": "/api/series_page"
    },
    {
      "source": "/movies/page/:page",
      "destination": "/api/movies_page?page=:page"
    },
    {
      "source": "/movies/page/:page/",
      "destination": "/api/movies_page?page=:page"
    },
    {
      "source": "/movies/:path",
      "destination": "/api/movies?path=:path"
    },
    {
      "source": "/movies/:path/",
      "destination": "/api/movies?path=:path"
    },
    {
      "source": "/movies",
      "destination": "/api/movies_page"
    },
    {
      "source": "/movies/",
      "destination": "/api/movies_page"
    },
    {
      "source": "/api/proxy",
      "destination": "/api/proxy"
    },
    {
      "source": "/:path*",
      "destination": "/api/proxy?path=/:path*"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "trailingSlash": false
}
